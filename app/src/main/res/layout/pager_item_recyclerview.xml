                            app:layout_constraintTop_toBottomOf="@+id/linearLayout11">

                            <TextView
                                android:id="@+id/tv_mood"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:dropDownHeight="fill_parent"
                                android:focusable="false"
                                android:text="Mood"
                                android:textColor="@color/white"
                                android:textSize="@dimen/size_text_no"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text=":"
                                android:textColor="@color/white"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/tv_description_mood"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:dropDownHeight="fill_parent"
                                android:focusable="false"
                                android:text="Diligent"
                                android:textColor="@color/white"
                                android:textSize="@dimen/size_text_no" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_123"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_10sdp"
                            android:orientation="horizontal"
                            android:weightSum="3"
                            app:layout_constraintTop_toBottomOf="@+id/linearLayout12">

                            <TextView
                                android:id="@+id/tv_color"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:dropDownHeight="fill_parent"
                                android:focusable="false"
                                android:text="Color"
                                android:textColor="@color/white"
                                android:textSize="@dimen/size_text_no"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text=":"
                                android:textColor="@color/white"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/tv_description_color"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:dropDownHeight="fill_parent"
                                android:focusable="false"
                                android:text="Silver"
                                android:textColor="@color/white"
                                android:textSize="@dimen/size_text_no" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_12"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_10sdp"
                            android:orientation="horizontal"
                            android:weightSum="3"
                            app:layout_constraintTop_toBottomOf="@+id/ll_123">

                            <TextView
                                android:id="@+id/tv_lucky_number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:focusable="false"
                                android:text="Lucky Number"
                                android:textColor="@color/white"
                                android:textSize="@dimen/size_text_no"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text=":"
                                android:textColor="@color/white"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/tv_description_lucky_number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:focusable="false"
                                android:text="3"
                                android:textColor="@color/white"
                                android:textSize="@dimen/size_text_no" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ll_13"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_10sdp"
                            android:orientation="horizontal"
                            android:paddingBottom="@dimen/_10sdp"
                            android:weightSum="3"
                            app:layout_constraintTop_toBottomOf="@+id/ll_12">

                            <TextView
                                android:id="@+id/tv_lucky_time"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:dropDownHeight="fill_parent"
                                android:focusable="false"
                                android:text="Lucky Time"
                                android:textColor="@color/white"
                                android:textSize="@dimen/size_text_no"
                                android:textStyle="bold" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text=":"
                                android:textColor="@color/white"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/tv_description_lucky_time"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="8am"
                                android:textColor="@color/white"
                                android:textSize="@dimen/size_text_no" />
                        </LinearLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>


                <RelativeLayout
                    android:id="@+id/relativeLayout2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_600sdp"
                    android:background="@color/black_blur"
                    android:visibility="@{data.isLoading ? View.VISIBLE : View.GONE}"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ProgressBar
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:backgroundTint="@color/red"
                        tools:ignore="UnusedAttribute" />
                </RelativeLayout>


                <androidx.cardview.widget.CardView
                    android:id="@+id/cv_bot_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_5sdp"
                    app:cardBackgroundColor="@color/black_blur"
                    app:cardCornerRadius="@dimen/_10sdp"
                    app:cardElevation="0dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cardView3">

                    <LinearLayout
                        android:id="@+id/homepage_redir_chat_layout"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:paddingBottom="20.0dip">

                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:src="@drawable/hp_redir_chat" />

                        <TextView
                            android:id="@+id/homepage_redir_chat_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15.0dip"
                            android:layout_marginRight="15.0dip"
                            android:paddingTop="11.0dip"
                            android:text="@string/menu_item1_txt"
                            android:textAllCaps="true"
                            android:textColor="@color/orange"
                            android:textSize="@dimen/size_text_title" />

                        <TextView
                            android:id="@+id/homepage_redir_chat_text"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15.0dip"
                            android:layout_marginTop="5.0dip"
                            android:layout_marginRight="15.0dip"
                            android:layout_marginBottom="15.0dip"
                            android:text="Do not keep those confusing thoughts to youself. \nOur best psychics are waiting to answer all your questions."
                            android:textColor="@color/white"
                            android:textSize="@dimen/size_text_no" />

                        <RelativeLayout
                            android:id="@+id/homepage_redir_chat_button"
                            android:layout_width="@dimen/size_width_button"
                            android:layout_height="@dimen/size_height_button"
                            android:layout_gravity="center"
                            android:background="@drawable/bg_sign_home"
                            android:gravity="center">

                            <TextView
                                android:id="@+id/homepage_redir_chat_button_text"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:maxLines="1"
                                android:text="A Doubt, A Question?"
                                android:textAllCaps="true"
                                android:textColor="@android:color/white"
                                android:textSize="@dimen/size_text_no"
                                tools:ignore="HardcodedText" />
                        </RelativeLayout>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>


                <LinearLayout
                    android:id="@+id/cv_bot_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="#00000000"
                    android:layout_marginTop="@dimen/_5sdp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cv_bot_1">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/homepage_redir_horoperso"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageView4"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:src="@drawable/number_row_background_bright"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30dp"
                            android:gravity="center"
                            android:padding="8.0dip"
                            android:text="@string/menu_item_24_txt"
                            android:textColor="@android:color/white"
                            android:textSize="20.0sp"
                            app:layout_constraintBottom_toBottomOf="@+id/imageView4"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                        <ImageView
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:layout_marginEnd="30dp"
                            android:src="@drawable/baseline_keyboard_arrow_right_white_36dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                    </androidx.constraintlayout.widget.ConstraintLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/dsagafh"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="#00000000"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cv_bot_2">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/homepage_redir_compat"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageView5"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:src="@drawable/number_row_background_dark"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30dp"
